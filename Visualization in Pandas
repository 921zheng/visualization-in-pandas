        "def highlight_inactive(value):\n",
        "  if isinstance(value,str):\n",
        "    return ''\n",
        "  if value <50:\n",
        "    return 'background-color:pink'\n",
        "  # return ''\n",
        "\n",
        "df.style.applymap(highlight_inactive)"
      ],
